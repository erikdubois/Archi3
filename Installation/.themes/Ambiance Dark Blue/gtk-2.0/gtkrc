# changes the x/ythickness
style "notebook" {
	bg[ACTIVE] = shade (0.97, @bg_color)
	bg[NORMAL] = shade (1.1, @bg_color)	# selected tab entire
	xthickness = 2
	ythickness = 2
		GtkNotebook::tab-overlap = -1
	GtkNotebook::tab-roundness = 0
	engine "murrine" {
		glazestyle = 1
		highlight_shade = 1.0
        	glow_shade = 2.0
        	gradient_shades     = {0.6, 0.8, 1.5, 1.5}
		border_shades = {1.5, 1.5}
		roundness=5
	}
}

